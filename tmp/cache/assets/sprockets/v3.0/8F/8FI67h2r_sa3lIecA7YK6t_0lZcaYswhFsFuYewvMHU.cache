  <form class="form-horizontal">

    <div class="control-group">
      <div class="controls">
        <h3>{{i18n 'user.change_email.title'}}</h3>
      </div>
    </div>

    {{#if success}}
        <div class="control-group">
          <div class="instructions">
            <p>{{i18n 'user.change_email.success'}}</p>
          </div>
        </div>
    {{else}}
      {{#if error}}
        <div class="control-group">
          <div class="instructions">
            <div class='alert alert-error'>{{errorMessage}}</div>
          </div>
        </div>
      {{/if}}

