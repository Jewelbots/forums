        {{#if taken}}
          {{i18n 'user.change_username.taken'}}
        {{/if}}
        <span>{{ errorMessage }}</span>
      </div>
    </div>

    <div class="control-group">
      <div class="controls">
        <button {{action "changeUsername"}} disabled={{saveDisabled}} class="btn btn-primary">{{saveButtonText}}</button>
          {{#if saved}}{{i18n 'saved'}}{{/if}}
      </div>
    </div>

  </form>
</section>
