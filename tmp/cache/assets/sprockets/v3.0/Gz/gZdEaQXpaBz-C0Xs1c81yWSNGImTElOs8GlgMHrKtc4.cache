  <div>
    <ul class='badge-groupings'>
    {{#each workingCopy as |wc|}}
      <li>
        {{#if wc.editing}}
          {{input value=wc.name}}
          <button {{action "save" wc}} class="btn no-text">{{fa-icon 'check'}}</button>
        {{else}}
          {{wc.displayName}}
        {{/if}}
        <div class='actions'>
          <button {{action "edit" wc}} class="btn no-text" disabled={{wc.system}}>{{fa-icon 'pencil'}}</button>
